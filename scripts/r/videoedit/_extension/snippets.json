{
  "md": {
    "prefix": "md",
    "body": ["{{ md('## $1', t='as', duration=3) }}"]
  },
  "audio_gap": {
    "prefix": "audio_gap",
    "body": ["{{ audio_gap(2, bgm_vol=0.5, fade_duration=0.25) }}"]
  },
  "tts": {
    "prefix": "tts",
    "body": ["{{ tts(True) }}"]
  },
  "bgm": {
    "prefix": "bgm",
    "body": ["{{ bgm('$1') }}"]
  },
  "crossfade": {
    "prefix": "crossfade",
    "body": ["{{ crossfade(0.2) }}"]
  },
  "comment": {
    "prefix": "comment",
    "body": ["{{ comment('$1', t='as') }}"]
  },
  "title": {
    "prefix": "title",
    "body": ["{{ slide('# $1\\n## $2', template='title', cf=0.2) }}"]
  },
  "slide": {
    "prefix": "slide",
    "body": ["{{ slide('$1', template='slide') }}"]
  },
  "video_end": {
    "prefix": "video_end",
    "body": ["{{ video_end(track='overlay', t='ae') }}"]
  },
  "cmd": {
    "prefix": "cmd",
    "body": ["{{ cmd('$1{sleep 1.5}\n', font_size=18) }}"]
  }
}
