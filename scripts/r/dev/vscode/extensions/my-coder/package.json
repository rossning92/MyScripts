{
  "name": "my-coder",
  "displayName": "my-coder",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "my-coder.edit",
        "title": "Coder: Edit"
      }
    ],
    "keybindings": [
      {
        "command": "my-coder.edit",
        "key": "ctrl+k c",
        "when": "editorTextFocus"
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.94.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x"
  }
}
